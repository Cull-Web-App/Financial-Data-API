{
  "name": "financial-data-api",
  "version": "1.0.0",
  "description": "Financial API to retrieve financial data from data providers",
  "main": "index.js",
  "scripts": {
    "debug-lambda-unix": "export SLS_DEBUG=* && node --inspect=5858 /usr/local/bin/serverless offline -s prod",
    "debug-lambda-win": "SET SLS_DEBUG=* && node --inspect=5858 %USERPROFILE%\\AppData\\Roaming\\npm\\node_modules\\serverless\\bin\\serverless offline -s dev",
    "lint": "npx eslint ./**/*.ts",
    "deploy": "npm run lint && sls deploy -s prod"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-lambda": "^1.0.5",
    "aws-sdk": "^2.685.0",
    "axios": "^0.19.2",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/axios": "^0.14.0",
    "@types/node": "^12.12.42",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.7.2",
    "serverless": "^1.71.3",
    "serverless-offline": "^4.9.4",
    "serverless-plugin-typescript": "^1.1.9",
    "typescript": "^3.9.3"
  }
}
